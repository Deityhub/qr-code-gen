{
  "name": "@deityhub/qr-code-gen",
  "version": "1.0.1",
  "description": "A robust, zero-dependency, highly configurable QR Code generator for your TypeScript/JavaScript projects.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./umd": "./dist/umd/qr-code-gen.min.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build:tsup": "tsup",
    "build:rollup": "rollup -c",
    "build": "npm run build:tsup && npm run build:rollup",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "qr",
    "qrcode",
    "generator",
    "typescript",
    "iso-18004",
    "browser",
    "node"
  ],
  "author": "Chizoba Jude Ojini <judoc96@gmail.com>",
  "homepage": "https://github.com/Deityhub/qr-code-gen#readme",
  "license": "MIT",
  "repository": "https://github.com/Deityhub/qr-code-gen",
  "bugs": {
    "url": "https://github.com/Deityhub/qr-code-gen/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.1",
    "rollup": "^4.53.3",
    "rollup-plugin-dts": "^6.3.0",
    "tslib": "^2.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.0.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
